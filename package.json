{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm ./src/server.ts",
    "start:dev": "nodemon",
    "start:prod": "npm run build && node ./dist/src/server.js",
    "build": "npx tsc",
    "prepare": "husky install",
    "test": "eslint src --fix && vitest run --changed HEAD~1",
    "coverage": "vitest run --coverage",
    "vitest": "vitest",
    "deploy": "eslint src --fix && vitest run --changed HEAD~1 && tsc && vite build && gh-pages -d dist",
    "cypress:open": "cypress open",
    "cypress:test": "nyc instrument --compact=false src instrumented && npx cypress run --e2e && npx nyc report && npx nyc report --reporter=text-summary",
    "cypress:coverage": "npx nyc instrument --compact=false src instrumented && npx nyc report --reporter"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.3.6",
    "babel-plugin-istanbul": "^6.1.1",
    "classnames": "^2.3.2",
    "express": "^4.18.2",
    "framer-motion": "^10.12.4",
    "istanbul-lib-coverage": "^3.2.0",
    "nyc": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "ts-node": "^10.9.1",
    "uuidv4": "^6.2.13",
    "vite-plugin-istanbul": "^4.0.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "cypress": "^12.10.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.3.1",
    "vitest": "^0.29.8"
  }
}
