{
  "name": "vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm ./src/server.ts",
    "start:dev": "nodemon",
    "start:prod": "npm run build && node ./dist/src/server.js",
    "build": "npx tsc",
    "prepare": "husky install",
    "test": "eslint src --fix && vitest run --changed HEAD~1",
    "coverage": "vitest run --coverage",
    "vitest": "vitest",
    "deploy": "eslint src --fix && vitest run --changed HEAD~1 && tsc && vite build && gh-pages -d dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/react-redux": "^7.1.25",
    "@types/uuidv4": "^5.0.0",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "framer-motion": "^10.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "ts-node": "^10.9.1",
    "uuidv4": "^6.2.13",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.0",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vitest": "^0.29.2"
  }
}
